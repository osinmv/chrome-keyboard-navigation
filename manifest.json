{
  "manifest_version": 3,
  "name": "vi-nav",
  "version": "1.0",
  "description": "My Keyboard Navigation Extension with Minimal Permissions",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "icon-insert.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
